<div>
    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
      {{#each coin as |coin| }}
    <tr>
      <th scope="row">{{coin.id}}</th>
      <td>{{coin.name}}</td>
      <td>{{coin.price}}</td> 
    </tr>
    {{/each}}
  </tbody>
</table>
</div>